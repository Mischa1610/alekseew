{
  "name": "alekseew",
  "version": "0.0.0",
  "description": "My monorepo for everything I want to develop, share, publish and much more.",
  "keywords": [
    "alekseew",
    "Angular",
    "Nest",
    "TypeScript",
    "NativeScript",
    "Cross-Platform"
  ],
  "homepage": "https://mischa1610.github.io/alekseew",
  "bugs": {
    "url": "https://github.com/Mischa1610/alekseew/issues"
  },
  "license": "MIT",
  "private": true,
  "author": "Michael Alekseew <alekseew.m@googlemail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mischa1610/alekseew.git"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.11.0"
  },
  "scripts": {
    "prepare-git-commit-template": "git config commit.template .gitmessage || exit 0",
    "postinstall": "npm run prepare-git-commit-template",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "commit": "git-cz",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/animations": "^8.2.11",
    "@angular/common": "^8.2.11",
    "@angular/compiler": "^8.2.11",
    "@angular/core": "^8.2.11",
    "@angular/forms": "^8.2.11",
    "@angular/platform-browser": "^8.2.11",
    "@angular/platform-browser-dynamic": "^8.2.11",
    "@angular/router": "^8.2.11",
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/microservices": "^6.8.3",
    "@nestjs/platform-express": "^6.8.3",
    "@nrwl/angular": "^8.6.0",
    "core-js": "^3.3.2",
    "document-register-element": "^1.14.3",
    "express": "^4.17.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.12",
    "@angular/cli": "8.3.12",
    "@angular/compiler-cli": "^8.2.11",
    "@angular/language-service": "^8.2.11",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.3",
    "@nrwl/cypress": "^8.6.0",
    "@nrwl/express": "^8.6.0",
    "@nrwl/jest": "^8.6.0",
    "@nrwl/nest": "^8.6.0",
    "@nrwl/node": "^8.6.0",
    "@nrwl/react": "^8.6.0",
    "@nrwl/schematics": "^8.6.0",
    "@nrwl/web": "^8.6.0",
    "@nrwl/workspace": "^8.6.0",
    "@testing-library/dom": "^6.8.1",
    "@testing-library/react": "^9.3.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "codelyzer": "^5.1.2",
    "commitizen": "^4.0.3",
    "cypress": "^3.4.1",
    "cz-conventional-changelog": "^3.0.2",
    "dotenv": "^8.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-preset-angular": "^7.1.1",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.5.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,html,css,scss,sass,less,md,yaml,sql}": [
      "npm run format:check --"
    ],
    "*.{ts,tsx,js,jsx}": [
      "npm run lint --"
    ]
  }
}
